Red [author: "Gregg Irwin"]

view [
	below
	space 2x2
	style button: button 125
	button "Counter"        [do %counter.red]
	button "Temperature"    [do %temperature.red]
	button "Timer"          [do %timer.red]
	button "CRUD"           [do %crud.red]
	button "Flight Booking" [do %flight-booking.red]
	button "Circle Drawer"  [do %circle-drawer.red]
	button "Cells"          ;[do %cells.red]
	pad 0x15
	button "Quit"           [quit]
]